{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "array",
    "items": {
        "type": "object",
        "properties": {
            "unique_id": {
                "type": "string"
            },
            "id": {
                "type": "string"
            },
            "name": {
                "type": "string"
            },
            "printings": {
                "type": "array",
                "items": {
                    "type": "object",
                    "properties": {
                        "unique_id": {
                            "type": "string"
                        },
                        "edition": {
                            "type": "string"
                        },
                        "start_card_id": {
                            "type": "string"
                        },
                        "end_card_id": {
                            "type": "string"
                        },
                        "initial_release_date": {
                            "type": ["string", "null"]
                        },
                        "out_of_print_date": {
                            "type": ["string", "null"]
                        },
                        "product_page": {
                            "type": ["string", "null"]
                        },
                        "collectors_center": {
                            "type": ["string", "null"]
                        },
                        "card_gallery": {
                            "type": ["string", "null"]
                        }
                    },
                    "required": [
                        "unique_id",
                        "edition",
                        "start_card_id",
                        "end_card_id",
                        "initial_release_date",
                        "out_of_print_date",
                        "product_page",
                        "collectors_center",
                        "card_gallery"
                    ]
                }
            }
        },
        "required": [
            "id",
            "name",
            "printings"
        ]
    }
}